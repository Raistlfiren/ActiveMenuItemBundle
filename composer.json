{
    "name": "bw/active-menu-item-bundle",
    "type": "library",
    "description": "The active menu item highlighting of simple HTML menu in Symfony applications",
    "keywords": ["bw", "active", "menu", "item", "highlight"],
    "homepage": "https://github.com/bocharsky-bw/ActiveMenuItemBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Victor Bocharsky",
            "email": "bocharsky.bw@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "twig/twig": "^2.0|^3.0"
    },
    "autoload": {
        "psr-4": { "BW\\ActiveMenuItemBundle\\": "src" }
    },
    "autoload-dev": {
        "psr-4": {
            "BW\\ActiveMenuItemBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.5.9",
        "symfony/framework-bundle": "^5.4",
        "symfony/twig-bundle": "^5.4",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^3.4"
    },
    "config": {
        "sort-packages": true
    }
}
