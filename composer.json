{
    "name": "bw/active-menu-item-bundle",
    "type": "library",
    "description": "The active menu item highlight of simple HTML menu for Symfony framework",
    "keywords": ["bw", "active", "menu", "item", "highlight"],
    "homepage": "https://github.com/bocharsky-bw/ActiveMenuItemBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Victor Bocharsky",
            "email": "bocharsky.bw@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/framework-bundle": "^3.4",
        "symfony/twig-bundle": "^3.4",
        "twig/twig": "^2.0"
    },
    "autoload": {
        "psr-4": { "": "src" }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
